{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    }
  },
  "globalEnv": ["JWT_ACCESS_SECRET", "JWT_REFRESH_SECRET"],
  "globalDependencies": [".env"]
}
